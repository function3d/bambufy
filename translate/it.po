msgstr "action:prompt_text Controllo MD5 di '{filename}'..."

msgid "Started MD5 check {filename}"
msgstr "Controllo MD5 di {filename} avviato"

msgid "action:prompt_text Processing '{filename}' ..."
msgstr "action:prompt_text Elaborazione di '{filename}'..."

msgid "action:prompt_begin Error"
msgstr "action:prompt_begin Errore"

msgid "action:prompt_text MD5 check failed. Print canceled."
msgstr "action:prompt_text Controllo MD5 fallito. Stampa annullata."

msgid ""
"Loading filament at {extruder_temp} for {extrude_len}mm at {speed/60} mm/s"
msgstr ""
"Caricamento filamento a {extruder_temp} °C per {extrude_len} mm a {speed/60} "
