msgstr "action:prompt_begin Цвет → Порт"

msgid "action:prompt_button LEVELING:ON|_IFS_COLORS_KAMP KAMP=0"
msgstr "action:prompt_button ВЫРАВН.:ON|_IFS_COLORS_KAMP KAMP=0"

msgid "action:prompt_button LEVELING:OFF|_IFS_COLORS_KAMP KAMP=1"
msgstr "action:prompt_button ВЫРАВН.:OFF|_IFS_COLORS_KAMP KAMP=1"

msgid ""
"action:prompt_button IFS:ON|_IFS_COLORS_NOTIFICATION TITLE='IFS' MSG='This "
"is a multicolor printing, IFS cannot be disabled.'"
msgstr ""
"action:prompt_button IFS:ON|_IFS_COLORS_NOTIFICATION TITLE='IFS' MSG='Это "
"многоцветная печать, IFS нельзя отключить.'"

msgid "action:prompt_button IFS:OFF|_IFS_COLORS_IFS EXTERNAL=0"
msgstr "action:prompt_button IFS:OFF|_IFS_COLORS_IFS EXTERNAL=0"

msgid "action:prompt_button IFS:ON|_IFS_COLORS_IFS EXTERNAL=1"
msgstr "action:prompt_button IFS:ON|_IFS_COLORS_IFS EXTERNAL=1"

msgid ""
"action:prompt_button BACKUP:OFF|_IFS_COLORS_NOTIFICATION TITLE='Backup' "
"MSG='This is a multicolor printing, BACKUP cannot be enabled.'"
msgstr ""
"action:prompt_button РЕЗЕРВ:OFF|_IFS_COLORS_NOTIFICATION TITLE='РЕЗЕРВ' "
"MSG='Это многоцветная печать, резервирование невозможно включить.'"

msgid ""
"action:prompt_button BACKUP:OFF|_IFS_COLORS_NOTIFICATION TITLE='Backup' "
"MSG='The BACKUP function is only possible with IFS enabled.'"
msgstr ""
"action:prompt_button Резерв:OFF|_IFS_COLORS_NOTIFICATION TITLE='Резерв' "
"MSG='Функция резервирования возможна только при включенном IFS.'"

msgid "action:prompt_button BACKUP:ON|_IFS_COLORS_BACKUP BACKUP=0"
msgstr "action:prompt_button РЕЗЕРВ:ON|_IFS_COLORS_BACKUP BACKUP=0"

msgid "action:prompt_button BACKUP:OFF|_IFS_COLORS_BACKUP BACKUP=1"
msgstr "action:prompt_button РЕЗЕРВ:OFF|_IFS_COLORS_BACKUP BACKUP=1"

msgid ""
"action:prompt_button P{port}:--|_IFS_COLORS_NOTIFICATION TITLE='Error' "
"MSG='IFS port P{port} empty.'"
msgstr ""
"action:prompt_button P{port}:--|_IFS_COLORS_NOTIFICATION TITLE='Ошибка' "
"MSG='Порт IFS P{port} пуст.'"

msgid ""
"action:prompt_button P{port}|_IFS_COLORS_NOTIFICATION TITLE='Error' "
"MSG='Incorrect filament type.'"
msgstr ""
"action:prompt_button P{port}|_IFS_COLORS_NOTIFICATION TITLE='Ошибка' "
"MSG='Несовместимый тип филамента.'"

msgid ""
"action:prompt_button P{port}|_IFS_COLORS_NOTIFICATION TITLE='Error' MSG='IFS "
"port P{port} empty.'"
msgstr ""
"action:prompt_button P{port}|_IFS_COLORS_NOTIFICATION TITLE='Ошибка' "
"MSG='Порт IFS P{port} пуст.'"

msgid ""
"action:prompt_text Update 'Filament change gcode' to v{ifs.min_version} or "
"higher"
msgstr ""
"action:prompt_text Обновите 'gcode смены филамента' до v{ifs.min_version} "
"или выше"

msgid ""
"Update 'Filament change gcode' to v{ifs.min_version} or higher https://"
"github.com/function3d/bambufy/blob/master/MACHINE_GCODE.md"
msgstr ""
"Обновите 'gcode смены филамента' до v{ifs.min_version} или выше: https://"
"github.com/function3d/bambufy/blob/master/MACHINE_GCODE.md"

msgid "action:prompt_footer_button Cancel|_IFS_COLORS_CANCEL"
msgstr "action:prompt_footer_button Отмена|_IFS_COLORS_CANCEL"

msgid "action:prompt_footer_button Print|_IFS_COLORS_PRINT|primary"
msgstr "action:prompt_footer_button Печать|_IFS_COLORS_PRINT|primary"

msgid ""
"action:prompt_footer_button Print|_IFS_COLORS_NOTIFICATION TITLE='Error' "
"MSG='Update Filament change gcode to v{ifs.min_version} or higher https://"
"github.com/function3d/bambufy/blob/master/MACHINE_GCODE.md'|error"
msgstr ""
"action:prompt_footer_button Печать|_IFS_COLORS_NOTIFICATION TITLE='Ошибка' "
"MSG='Обновите gcode смены филамента до v{ifs.min_version} или выше: https://"
"github.com/function3d/bambufy/blob/master/MACHINE_GCODE.md'|error"

msgid ""
"action:prompt_footer_button Print|_IFS_COLORS_NOTIFICATION TITLE='Error' "
"MSG='It is not possible to print until all colors have been assigned.'|error"
msgstr ""
"action:prompt_footer_button Печать|_IFS_COLORS_NOTIFICATION TITLE='Ошибка' "
"MSG='Печать невозможна, пока всем цветам не присвоены порты.'|error"

msgid ""
"action:prompt_button Select for C{tool|int + 1}:{types[tool|int]}|||"
"{colors[tool|int]}"
msgstr ""
"action:prompt_button Выбрать для C{tool|int + 1}:{types[tool|int]}|||"
"{colors[tool|int]}"

msgid ""
"action:prompt_button P{port}:{ifs_types[port-1]}|_IFS_COLORS_NOTIFICATION "
"TITLE='Error' MSG='Incorrect filament type.'"
msgstr ""
"action:prompt_button P{port}:{ifs_types[port-1]}|_IFS_COLORS_NOTIFICATION "
"TITLE='Ошибка' MSG='Несовместимый тип филамента.'"

msgid "Reinstalling bambufy..."
msgstr "Переустановка Bambufy…"

msgid "action:prompt_begin Warning"
msgstr "action:prompt_begin Предупреждение"

msgid ""
"action:prompt_text Bambufy does not work with the native screen, it requires "
"Guppy Screen (DISPLAY_OFF)"
msgstr ""
"action:prompt_text Bambufy не работает с родным экраном, требуется Guppy "
