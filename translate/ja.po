msgstr "action:prompt_button レベリング:OFF|_IFS_COLORS_KAMP KAMP=1"

msgid ""
"action:prompt_button IFS:ON|_IFS_COLORS_NOTIFICATION TITLE='IFS' MSG='This "
"is a multicolor printing, IFS cannot be disabled.'"
msgstr ""
"action:prompt_button IFS:ON|_IFS_COLORS_NOTIFICATION TITLE='IFS' MSG='マルチ"
"カラー印刷中です IFSを無効にできません'"

